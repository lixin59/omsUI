{
  "name": "omsUI",
  "version": "2.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "add:husky-config": "npx husky add .husky/pre-commit 'npm run lint-staged'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "yarn version",
    "serve": "vite preview",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branch": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ]
    ],
    "preset": "angular"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint",
    "**/*.{js,jsx,tsx,ts,less,md}": [
      "prettier --write"
    ]
  },
  "author": {
    "name": "lixin"
  },
  "description": "webShell",
  "repository": {
    "url": "https://github.com/lixin59/omsUI"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "@monaco-editor/react": "^4.4.6",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "@mui/x-data-grid": "^5.13.0",
    "@mui/x-data-grid-generator": "^5.13.0",
    "@rjsf/core": "^4.2.2",
    "@rjsf/material-ui": "^4.2.2",
    "axios": "^0.24.0",
    "chonky": "^2.3.0",
    "chonky-icon-fontawesome": "^2.3.0",
    "echarts": "^5.2.2",
    "echarts-for-react": "^3.0.2",
    "echarts-liquidfill": "^3.1.0",
    "immutability-helper": "^3.1.1",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "notistack": "^2.0.2",
    "qs": "^6.10.1",
    "react": "^17.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^17.0.0",
    "react-markdown": "8.0.3",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.1",
    "react-router-dom": "6.0.2",
    "react-syntax-highlighter": "^15.4.5",
    "react-vnc": "^0.5.1",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "xterm": "^4.19.0",
    "xterm-addon-attach": "^0.6.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-web-links": "^0.6.0"
  },
  "devDependencies": {
    "@types/js-base64": "^3.3.1",
    "@types/lodash": "^4.14.172",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "semantic-release": "^20.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
